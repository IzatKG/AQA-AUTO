+package UI.codingBat;
+
+import java.util.HashMap;
+import java.util.Map;
+
+public class TasksCodingBat {
+    // Измените и верните заданную карту следующим образом:
+    // если ключ «a» имеет значение, установите ключ «b» на это значение,
+    // а ключ «a» на значение «».
+    // По сути, «b» — это хулиган, который берет значение и заменяет его пустой строкой.
+    public static
+    Map<String,String>
+    mapBully(Map<String,String>map){
+        if (map.containsKey("a") && map.get("a") !=null){
+            map.put("b",map.get("a"));
+            map.put("a","");
+        }
+        return map;
+    }
+    public static void main(String[] args) {
+        Map<String,String> exampleMap= new HashMap<>();
+        exampleMap.put("a","candy");
+        exampleMap.put("d","dirt");
+        Map<String,String> result = mapBully(exampleMap);
+        System.out.println(result);
+
+    }
+}
